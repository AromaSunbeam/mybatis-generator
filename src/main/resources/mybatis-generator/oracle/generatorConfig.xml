        <jdbcConnection driverClass="oracle.jdbc.driver.OracleDriver"
                        connectionURL="jdbc:oracle:thin:@dlks-pc:1521:orcl" userId="examcenter"
                        password="123456"/>
       <!--  <javaTypeResolver >
            <property name="forceBigDecimals" value="false" />
        </javaTypeResolver> -->
        
        <javaModelGenerator targetPackage="com.bshinfo.entity"
